{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "json-giant"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "electron-entry-point.js",
        { "glob": "**/*", "input": "../node_modules/bootstrap/dist/css/", "output": "./node_assets/"},
        { "glob": "**/*", "input": "../node_modules/font-awesome/css/", "output": "./node_assets/"},
        { "glob": "**/*", "input": "../node_modules/font-awesome/fonts/", "output": "./fonts/"},
        { "glob": "**/*", "input": "../node_modules/jquery/dist/", "output": "./node_assets/"},
        { "glob": "**/*", "input": "../node_modules/jquery/dist/", "output": "./node_assets/"},
        { "glob": "**/*", "input": "../node_modules/tether/dist/js/", "output": "./node_assets/"},
        { "glob": "**/*", "input": "../node_modules/popper.js/dist/umd/", "output": "./node_assets/"},
        { "glob": "**/*", "input": "../node_modules/bootstrap/dist/js/", "output": "./node_assets/"}
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss"
      ],
      "stylePreprocessorOptions": [
        "./"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
